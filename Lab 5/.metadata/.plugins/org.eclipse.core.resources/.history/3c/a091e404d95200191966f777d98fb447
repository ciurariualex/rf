package rf;

public class DistanceForm {
	public Form form1;
	public Form form2;
	public double distance;
	
	public DistanceForm(Form form1, Form form2, double distance) {
		this.form1 = form1;
		this.form2 = form2;
		this.distance = distance;
	}
	
	public double getDistance() {
	    return distance;
	  }
	  
	  public void setDistance(double distance) {
	    this.distance = distance;
	  }
	  
	@Override
	  public int compareTo(DistanceForm d) {
	    if (getCreatedOn() == null || u.getCreatedOn() == null) {
	      return 0;
	    }
	    return getCreatedOn().compareTo(u.getCreatedOn());
	  }
}
